{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "commonjs",
    "lib": ["es2020", "dom", "dom.iterable"],
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./types", "node_modules/@types"],
    "strict": true,
    "downlevelIteration": true
  },
  "include": ["types", "./src/**/*", "sw.js"],
  "exclude": ["node_modules"]
}
